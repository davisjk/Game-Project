  room_goto(rm_start);
}

if (button_text == "UPGRADES") {
  global.next_room = 1;
  room_goto(rm_upgrades);
}

if (button_text == "SOUND: ON") {
  global.sound_enabled = false;
  button_text = "SOUND: OFF";
}

else if (button_text == "SOUND: OFF") {
  global.sound_enabled = true;
  button_text = "SOUND: ON";
}

if (button_text == "FPS: ON") {
  global.fps_enabled = false;
  button_text = "FPS: OFF";
}

else if (button_text == "FPS: OFF") {
  global.fps_enabled = true;
  button_text = "FPS: ON";
}

else if (button_text == "CONTINUE") {
    if(global.next_room == 1) // go to level 1
    {
        room_goto(rm_level1);
    }
    else if(global.next_room == 2) // go to level 2
    {
        room_goto(rm_level2);
    }
    else if(global.next_room == 3) // go to level 3
    {
        room_goto(rm_level3);
    }
    else // go to lose screen
    {
        room_goto(rm_end);
    }
}
